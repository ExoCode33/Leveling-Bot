{
  "name": "one-piece-xp-bot",
  "version": "3.1.0",
  "description": "Clean One Piece themed XP tracking bot with canvas generation, Redis caching, and automatic database maintenance",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "echo \"No tests specified\" && exit 0",
    "setup": "npm install && npm run setup:redis",
    "setup:redis": "node scripts/setup-redis.js",
    "setup:docker-redis": "docker run -d --name redis-leveling-bot -p 6379:6379 --restart unless-stopped redis:alpine",
    "redis:start": "redis-server",
    "redis:test": "redis-cli ping",
    "redis:stop": "redis-cli shutdown",
    "cache:stats": "node scripts/cache-stats.js",
    "health": "node scripts/health-check.js",
    "maintenance": "node scripts/database-maintenance.js run",
    "maintenance:dry-run": "node scripts/database-maintenance.js dry-run",
    "cleanup": "node scripts/database-maintenance.js run",
    "cleanup:preview": "node scripts/database-maintenance.js dry-run"
  },
  "keywords": [
    "discord",
    "bot",
    "xp",
    "leveling",
    "one-piece",
    "canvas",
    "redis",
    "caching",
    "database-maintenance"
  ],
  "author": "ExoCode",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.16.0",
    "pg": "^8.12.0",
    "dotenv": "^16.4.5",
    "canvas": "^2.11.2",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
